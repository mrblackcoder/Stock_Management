# Environment Variables Template
# Copy this file to .env.development or .env.production and fill in the values
# Note: Create React App requires REACT_APP_ prefix for environment variables

# API Backend URL
REACT_APP_API_URL=http://localhost:8080/api

# Application Information
REACT_APP_APP_NAME=Stock Management System
REACT_APP_APP_VERSION=1.0.0
REACT_APP_ENVIRONMENT=development

# Optional: Encryption Key (for token storage)
# REACT_APP_ENCRYPTION_KEY=your_secure_key_here
